<p class="notice"><%= notice %></p>
<fieldset class="assmnt-show">
	<legend>Answer</legend>
  	<table>
  		<tr>
  	  <th>Sequence:</th>
  	  <td><%= @answer.sequence %></td>
  	</tr>

  		<tr>
  	  <th>Answer:</th>
  	  <td><%= @answer.answer %></td>
  	</tr>

  		<tr>
  	  <th>Shortname:</th>
  	  <td><%= @answer.shortname %></td>
  	</tr>

  		<tr>
  	  <th>Value:</th>
  	  <td><%= @answer.value %></td>
  	</tr>

  		<tr>
  	  <th>Requires other:</th>
  	  <td><%= @answer.requires_other %></td>
  	</tr>

  		<tr>
  	  <th>Other question:</th>
  	  <td><%= @answer.other_question %></td>
  	</tr>

  		<tr>
  	  <th>Answer eval:</th>
  	  <td><%= @answer.answer_eval %></td>
  	</tr>


	</table>
</fieldset>

<div class="actions">
  <span class="crud">
  	<%= link_to 'Edit', edit_answer_path(@answer) %> |
  </span>
  <span class="breadcrumbs f-right">
  <%= link_to 'Assessments', assessments_path %>
  <%= link_to 'Assessment',  assessment_path(@answer.question.assessment_id) %> 
  <%= link_to 'Question',  question_path(@answer.question_id) %> 

  </span>
</div>