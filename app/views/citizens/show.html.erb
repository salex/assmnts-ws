<fieldset class="form-list">
	<legend>User</legend>
  <table >
	  <tr>
      <th>Login:</th>
      <td><%= @citizen.login %></td>
    </tr>
	  <tr>
      <th>Name:</th>
      <td><%= @citizen.name_full %></td>
    </tr>
	  <tr>
      <th>eMail:</th>
      <td><%= @citizen.email %></td>
    </tr>
	  <tr>
      <th>Phone:</th>
      <td><%= number_to_phone(@citizen.phone_primary.to_i) %></td>
    </tr>
	  <tr>
      <th>Phone Mobile:</th>
      <td><%= number_to_phone(@citizen.phone_secondary.to_i) %></td>
    </tr>
	  <tr>
      <th>Sign-in Count:</th>
      <td><%= @citizen.sign_in_count %></td>
    </tr>
  </table>
</fieldset>
<div class="actions navspan">
<%= link_to 'Edit', edit_citizen_path(@citizen) %> |
</div>